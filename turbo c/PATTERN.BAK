

// count the digits in a number....

#include<math.h>



void main()
{
	int j,i,f=1;
	clrscr();
	for(j=2;j<=250;j++)
	{
		f=1;
		for(i=2;i<j;i++)
		{
			if(j%i==0)
			{
				f=0;
				break;
			}
		}

		if(f==1)
			printf("%d\t",j);

	}

	getch();
}




